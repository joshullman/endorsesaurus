<!DOCTYPE html>
<html>
<head>
  <title>Endorsesaurus</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link href='https://fonts.googleapis.com/css?family=Amatic+SC:400,700' rel='stylesheet' type='text/css'>
</head>
<body>

<section class="mbr-navbar mbr-navbar--freeze mbr-navbar--absolute mbr-navbar--transparent mbr-navbar--sticky mbr-navbar--auto-collapse" id="menu-41">
    <div class="mbr-navbar__section mbr-section">
        <div class="mbr-section__container container">
            <div class="mbr-navbar__container">
                <div class="mbr-navbar__column mbr-navbar__column--s mbr-navbar__brand">
                    <span class="mbr-navbar__brand-link mbr-brand mbr-brand--inline">
                        <span class="mbr-brand__logo"><a href="https://mobirise.com/bootstrap-template/"><img class="mbr-navbar__brand-img mbr-brand__img" src="assets/img/logo.png" alt="Mobirise"></a></span>
                        <span class="mbr-brand__name"><a class="mbr-brand__name text-white" href="https://mobirise.com/bootstrap-template/">Endorsesaurus</a></span>
                    </span>
                </div>
                <div class="mbr-navbar__hamburger mbr-hamburger text-white"><span class="mbr-hamburger__line"></span></div>
                <div class="mbr-navbar__column mbr-navbar__menu">
                    <nav class="mbr-navbar__menu-box mbr-navbar__menu-box--inline-right">
                        <div class="mbr-navbar__column"><ul class="mbr-navbar__items mbr-navbar__items--right mbr-buttons mbr-buttons--freeze mbr-buttons--right btn-decorator mbr-buttons--active"><li class="mbr-navbar__item"><a class="mbr-buttons__link btn text-white" href="movies.html">MOVIES</a></li><li class="mbr-navbar__item"><a class="mbr-buttons__link btn text-white" href="shows.html">SHOWS</a></li> <li class="mbr-navbar__item"><a class="mbr-buttons__link btn text-white" href="tags.html">TAGS</a></li></ul></div>
                        <div class="mbr-navbar__column">
                            <ul class="mbr-navbar__items mbr-navbar__items--right mbr-buttons mbr-buttons--freeze mbr-buttons--right btn-inverse mbr-buttons--active">
                                <li class="mbr-navbar__item dropdown">
                                  <a class="mbr-buttons__btn btn btn-default dropdown-toggle" data-toggle="dropdown" href="#">Current User
                                  <span class="caret"></span></a>
                                  <ul class="dropdown-menu">
                                    <li class="mbr-navbar__item"><a class="mbr-buttons__btn" href="dashboard.html">Dashboard</a></li>
                                    <li class="mbr-navbar__item"><a class="mbr-buttons__btn" href="profile.html">Profile</a></li>
                                    <li class="mbr-navbar__item"><a class="mbr-buttons__btn" href="friends.html">Friends</a></li>
                                    <li class="mbr-navbar__item"><a class="mbr-buttons__btn" href="movies.html">Movies</a></li>
                                    <li class="mbr-navbar__item"><a class="mbr-buttons__btn" href="shows.html">Shows</a></li>
                                  </ul>
                                </li>
                            </ul>
                        </div>


                    </nav>
                </div>
            </div>
        </div>
    </div>
</section>



<%= yield %>

<footer class="mbr-section mbr-section--relative mbr-section--fixed-size" id="footer1-76" style="background-color: rgb(68, 68, 68);">
    
    <div class="mbr-section__container container">
        <div class="mbr-footer mbr-footer--wysiwyg row">
            <div class="col-sm-12">
                <p class="mbr-footer__copyright"></p><p>Copyright (c) 2015 Company Name. <a href="https://mobirise.com/bootstrap-template/license.txt" class="text-gray">License</a></p><p></p>
            </div>
        </div>
    </div>
</footer>

<script>
  window.fbAsyncInit = function() {
    FB.init({
      appId      : '170206153377410',
      xfbml      : true,
      version    : 'v2.6'
    });
  };

  (function(d, s, id){
     var js, fjs = d.getElementsByTagName(s)[0];
     if (d.getElementById(id)) {return;}
     js = d.createElement(s); js.id = id;
     js.src = "//connect.facebook.net/en_US/sdk.js";
     fjs.parentNode.insertBefore(js, fjs);
   }(document, 'script', 'facebook-jssdk'));
</script>

</body>
</html>
